package br.com.javaoito.list;

import java.io.BufferedReader;
import java.io.FileReader;
import java.io.IOException;
import java.util.List;
import java.util.stream.Collectors;

public class WorkingWithList {
	
	public List<String> readFile() throws IOException {
		
		BufferedReader file = new BufferedReader(new FileReader("/home/adriano.silva/√Årea de Trabalho/input.csv"));
		
		System.out.println(file.lines().collect(Collectors.joining(System.lineSeparator())));
		
		
		
		return file.lines().collect(Collectors.joining(System.lineSeparator()))
	}

}
